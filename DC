package laboratornaya1;

import java.lang.*;
import java.util.*;
import java.math.*;

public class MyClass {
public static void main(String[] args) {
int a,b,c;
a=5;
b=2;
c=1;
double x1,x2;
double d = Math.sqrt(b*b-4*a*c);
x1=(-b+Math.sqrt(d))/(2*a);
x2=(-b-Math.sqrt(d))/(2*a);
System.out.println( "x1= " + x1 + " , x2= " + x2 );
}
}
